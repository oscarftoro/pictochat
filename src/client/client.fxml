<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="168.0" prefWidth="240.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml" fx:controller="client.ClientController">
  <children>
    <Label layoutX="14.0" layoutY="14.0" text="Client">
      <font>
        <Font name="Free Monospaced Bold" size="26.0" />
      </font>
    </Label>
    <HBox id="HBox" alignment="CENTER" layoutX="14.0" layoutY="54.0" spacing="5.0">
      <children>
        <Label text="Port" />
        <TextField fx:id="port_txt_field" prefWidth="45.0" promptText="7890" />
      </children>
    </HBox>
    <HBox id="HBox" alignment="CENTER" layoutX="15.0" layoutY="82.0" spacing="5.0">
      <children>
        <Label text="URL" />
        <TextField fx:id="url_txt_field" prefWidth="125.0" promptText="127.0.0.1" />
      </children>
    </HBox>
    <Button fx:id="connect_bttn" layoutX="15.0" layoutY="119.0" mnemonicParsing="false" onAction="#handleConnectButton" text="Connect" />
  </children>
  <stylesheets>
    <URL value="@../datw12chat/sample.css" />
  </stylesheets>
</AnchorPane>
